# Google Cloud Project Configuration
PROJECT_ID=YOUR_PROJECT_ID
LOCATION=us               # Document AI location (use 'us' for processors in the US multi-region)
GOOGLE_APPLICATION_CREDENTIALS=./pdf2csv-converter-tool-ee510faebb5c.json

# Cloud Storage Configuration (Your Two Buckets)
INPUT_BUCKET_NAME=pdf-data-extraction-input-bucket
OUTPUT_BUCKET_NAME=pdf-data-extraction-output-bucket
INPUT_FOLDER=input-pdf
OUTPUT_FOLDER=doc-ai-output

# Document AI Processor IDs (will be set automatically by deployment script)
CUSTOM_PROCESSOR_ID=PLACEHOLDER

# Processing Configuration (Document AI only)
ENABLE_PREPROCESSING=false

# Mobile number validation (Australian format: 04xxxxxxxx)
MOBILE_PATTERN=^04\\d{8}$

# Local directories (for temporary processing)
OUTPUT_DIRECTORY=csv_outputs
ENHANCED_PDF_DIRECTORY=enhanced_pdfs
LOG_DIRECTORY=processing_logs

# Batch processing settings
MAX_CONCURRENT_REQUESTS=5
RETRY_ATTEMPTS=3
RETRY_DELAY=2

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Field Mapping Configuration
FIELD_MAPPING_NAME=full_name
FIELD_MAPPING_DOB=date_of_birth
FIELD_MAPPING_ADDRESS=address
FIELD_MAPPING_MOBILE=mobile
FIELD_MAPPING_EMAIL=email
FIELD_MAPPING_LANDLINE=landline
FIELD_MAPPING_LAST_SEEN=last_seen

# Required fields (records without these will be skipped)
REQUIRED_FIELDS=mobile

# CSV Output Configuration
CSV_ENCODING=utf-8
CSV_SEPARATOR=,
INCLUDE_HEADER=true

# Performance Monitoring
ENABLE_METRICS=true
METRICS_OUTPUT_FILE=processing_metrics.json

# Database Configuration (Google Cloud SQL)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=pdf2csv_db
DB_USER=pdf2csv_user
DB_PASSWORD=@Sharing1234
DB_SSL=true
DB_SOCKET_PATH=/cloudsql/YOUR_PROJECT_ID:us-central1:pdf2csv-db